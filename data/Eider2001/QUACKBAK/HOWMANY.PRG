clear
SET talk OFF
SET EXACT OFF
CLOSE ALL
USE VISIT
SET FILTER TO NEST_NO<>'TUT'
COUNT FOR .NOT. CHECK TO TOTAL
COUNT FOR .NOT. CHECK .AND. NEXTVISIT > CTOD('01/01/99') TO ACTIVE
COUNT FOR SPECIES = 'NOPI' .AND. .NOT. CHECK TO nopi
COUNT FOR (SPECIES = 'NOPI'.and. status<> 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO actvnopi
COUNT FOR (SPECIES = 'NOPI'.and. status= 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO NOPIBRD
COUNT FOR SPECIES = 'SPEI' .AND. .NOT. CHECK TO SPEI
COUNT FOR (SPECIES = 'SPEI'.and. status<> 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO actvspei
COUNT FOR (SPECIES = 'SPEI'.and. status= 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO speiBRD
COUNT FOR SPECIES = 'SPEI' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO actvSPEI
COUNT FOR SPECIES = 'OLDS' .AND. .NOT. CHECK TO OLDS
COUNT FOR SPECIES = 'OLDS' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO actvOLDS
COUNT FOR SPECIES = 'COEI' .AND. .NOT. CHECK TO COEI
COUNT FOR SPECIES = 'COEI' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO actvCOEI
COUNT FOR SPECIES = 'GRSC' .AND. .NOT. CHECK TO GRSC
COUNT FOR SPECIES = 'GRSC' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/99')) TO actvGRSC
SUCCESS=ACTIVE/TOTAL
NOPISUC=actvnopi/nopi
? 'TOTAL NESTS:  '
?? TOTAL
? '  ACTIVE NESTS:  '
?? ACTIVE
? '  APPARENT SUCCESS:  '
?? SUCCESS
?
? 'TOTAL NOPI:  '
?? NOPI
? '   ACTIVE NOPI:  '
?? actvnopi
? '   NOPI BROODS ACTIVE:  '
?? nopibrd
? '   PINTAIL SUCCESS:  '
?? NOPISUC
?
?'TOTAL SPEI:  '
?? spei
?'  ACTIVE SPEI:  '
?? actvspei
?'  SPEI BROODS ACTIVE:  '
??speibrd
?
?'TOTAL OLDS:  '
?? olds
?'  ACTIVE OLDS:  '
?? actvolds
?
?'TOTAL COEI:  '
?? coei
?'  ACTIVE COEI:  '
?? actvcoei
?'TOTAL GRSC:  '
?? grsc
?'  ACTIVE GRSC:  '
?? actvgrsc
return
