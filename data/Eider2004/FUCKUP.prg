clear
USE visit
INDEX ON NEST_NO + DTOC(DATE) TAG DATETIME
set order TO DATETIME
@ 10,10 say 'USE THIS PROGRAM TO REMOVE LINES FROM THE VISIT FILE'
WAIT
BROWS
PACK
SET CONS OFF

GOTO TOP
DO WHILE .NOT. EOF()
	mnest = nest_no
	DO WHILE nest_no = mnest
		IF .NOT. check
		REPL check WITH .T.
		ENDIF
		SKIP
	ENDDO
	SKIP -1
		REPL check WITH .F.
	SKIP
ENDDO
