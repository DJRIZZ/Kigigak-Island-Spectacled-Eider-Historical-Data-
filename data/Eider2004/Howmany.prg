clear
SET talk OFF
SET EXACT OFF
CLOSE ALL
USE VISIT

COUNT FOR .NOT. CHECK TO TOTAL
COUNT FOR .NOT. CHECK .AND. NEXTVISIT > CTOD('01/01/2003') TO ACTIVE
COUNT FOR SPECIES = 'PALO' .AND. .NOT. CHECK TO PALO
COUNT FOR (SPECIES = 'PALO'.and. status<> 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO actvPALO
COUNT FOR (SPECIES = 'PALO'.and. status= 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO PALOBRD
COUNT FOR SPECIES = 'SPEI' .AND. .NOT. CHECK TO SPEI
COUNT FOR (SPECIES = 'SPEI'.and. status<> 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO actvspei
COUNT FOR (SPECIES = 'SPEI'.and. status= 'BROOD') .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO speiBRD
COUNT FOR SPECIES = 'SPEI' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO actvSPEI
COUNT FOR SPECIES = 'OLDS' .AND. .NOT. CHECK TO OLDS
COUNT FOR SPECIES = 'OLDS' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO actvOLDS
COUNT FOR SPECIES = 'COEI' .AND. .NOT. CHECK TO COEI
COUNT FOR SPECIES = 'COEI' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO actvCOEI
COUNT FOR SPECIES = 'RTLO' .AND. .NOT. CHECK TO RTLO
COUNT FOR SPECIES = 'RTLO' .AND. (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) TO actvRTLO
SUCCESS=ACTIVE/TOTAL
PALOSUC=actvPALO/PALO
COEISUC=actvCOEI/COEI
SPEISUC=actvSPEI/SPEI
? 'ACTIVE NESTS:  '
?? ACTIVE
? 'TOTAL NESTS:     '
?? TOTAL
?
*? '  APPARENT SUCCESS:  '
*?? SUCCESS
*?

?'  ACTIVE SPEI:  '
?? actvspei
?'  TOTAL SPEI:     '
?? spei
? '   SPEC EIDER SUCCESS:  '
?? SPEISUC
?
*?'  SPEI BROODS ACTIVE:  '
*??speibrd
*?

?'  ACTIVE COEI:  '
?? actvcoei
?'  TOTAL COEI:     '
?? coei
? '   COMMON EIDER SUCCESS:  '
?? COEISUC
?
?'  ACTIVE OLDS:  '
?? actvolds
?'  TOTAL OLDS:     '
?? olds
?

?'  ACTIVE RTLO:  '
?? actvRTLO
?'  TOTAL RTLO:     '
?? RTLO
?
? ' ACTIVE PALO:  '
?? actvPALO
? ' TOTAL PALO:     '
?? PALO

*? '   PALO BROODS ACTIVE:  '
*?? PALObrd
*? '   PINTAIL SUCCESS:  '
*?? PALOSUC
*?

SET SAFETY OFF
SET FILTER TO (.NOT. CHECK .AND. NEXTVISIT > CTOD('1/1/2003')) 
COPY TO ACTIVE
SET SAFETY ON
return